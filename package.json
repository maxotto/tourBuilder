{
  "name": "tourbuilder",
  "version": "1.0.0",
  "description": "krpano custom tour builder",
  "main": "build.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "krpano"
  ],
  "nodemonConfig": {
    "delay": "2500",
    "verbose": true,
    "watch": [
      "templates",
      "planEditor",
      "templatePWA1"
    ],
    "ignore": [
      "planEditor/public/*.*",
      "planEditor/public/js/*.*",
      "planEditor/public/css/*.*",
      "planEditor/storage/*.*",
      "planEditor/node_modules/*.*",
      "planEditor/client/node_modules/*.*"
    ],
    "ext": "js,html,css,vue,json",
    "exec": "node planBuilderDev.js"
  },
  "author": "Andrey Mikhailichenko",
  "license": "ISC",
  "dependencies": {
    "express-session": "^1.15.6",
    "fs-extra": "^7.0.0",
    "fstream": "^1.0.11",
    "ftp-deploy": "^2.1.3",
    "mongoose": "^5.3.7",
    "npm": "^6.4.1",
    "xml2js": "^0.4.19"
  }
}
